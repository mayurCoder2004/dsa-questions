import java.util.*;
class Main {
    public static void main(String[] args) {
        String s = "-9/*+5346";
        Stack<String> val = new Stack<>();
        
        int n = s.length();
        
        for (int i=n-1; i>=0; i--) {
            char ch = s.charAt(i);
            int ascii = (int) ch;
            if(ascii >= 48 && ascii <= 57) {
                val.push(ch + "");
            }
            else {
                String v1 = val.pop();
                String v2 = val.pop();
                String infix = "(" + v1 + ch + v2 + ")";
                val.push(infix);
            }
        }
        System.out.println(val.peek());
    }
}
