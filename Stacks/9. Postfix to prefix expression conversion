import java.util.*;
class Main {
    public static void main(String[] args) {
        String s = "953+4*6/-";
        Stack<String> val = new Stack<>();
        
        int n = s.length();
        
        for (int i=0; i<n; i++) {
            char ch = s.charAt(i);
            int ascii = (int) ch;
            if(ascii >= 48 && ascii <= 57) {
                val.push(String.valueOf(ch));
            }
            else {
                String v2 = val.pop();
                String v1 = val.pop();
                String op = String.valueOf(ch);
                String exp = op + v1 + v2;
                val.push(exp);
            }
        }
        System.out.println(val.peek());
    }
}
