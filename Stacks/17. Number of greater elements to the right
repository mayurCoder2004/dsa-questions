                                                            // BRUTE FORCE APPROACH


class Solution {
    public static int[] count_NGE(int arr[], int indices[]) {
        // code here
        int n = arr.length;
        int [] ge = new int[indices.length];
        
        for (int i=0; i<indices.length; i++) {
            int idx = indices[i];
            int count = 0;
            for (int j=0; j<n; j++) {
                if (arr[j] == arr[idx]) {
                    for (int k=j+1; k<n; k++) {
                        if (arr[j] < arr[k]) {
                            count++;
                        }
                    }
                }
            }
            ge[i] = count;
        }
        return ge;
    }
}


                                                                    // BETTER APPROACH


class Solution {
    public static int[] count_NGE(int arr[], int indices[]) {
        // code here
        int n = arr.length;
        int [] ge = new int[indices.length];
        
        for (int i=0; i<indices.length; i++) {
            int idx = indices[i];
            int count = 0;
            
            for (int j = idx + 1; j < n; j++) {
                if (arr[j] > arr[idx]) {
                    count++;
                }
            }
            
            ge[i] = count;
        }
        return ge;
    }
}

