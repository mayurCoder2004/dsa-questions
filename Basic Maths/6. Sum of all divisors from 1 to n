class Solution {
    public static int factorial(int n){
        int fact = 1;
        for (int i=1; i<=n; i++){
            fact *= i;
        }
        return fact;
    }
    public static int sumOfDivisors(int n) {
        int sum = 0;
        for (int i=1; i<=n; i++){
            sum += factorial(i);
        }
        return sum;
    }
}
