class Solution {
    public int findContentChildren(int[] g, int[] s) {
        int n1 = g.length;
        int n2 = s.length;
        int count = 0;
        Arrays.sort(g);
        Arrays.sort(s);

        int i = n1-1, j = n2-1;

        while (i >= 0 && j >= 0) {
            if (g[i] <= s[j]) {
                count++;
                j--;
            }
            i--;
        }
        return count;
    }
}
