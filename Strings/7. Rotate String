class Solution {
    public String rotate(String s, int k) {
        int n = s.length();
        k = k % n;

        return s.substring(n-k) + s.substring(0, n-k);
    }
    public boolean rotateString(String s, String goal) {
        int n = s.length();
        for (int i=1; i<=n; i++) {
            String rotate = rotate(s, i);
            if (rotate.equals(goal)) {
                return true;
            }
        }
        return false;
    }
}
