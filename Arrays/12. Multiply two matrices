public class Solution{
    public static int [][] multiply(int [][] a, int r1, int c1, int [][] b, int r2, int c2){
        int [][] c = new int[r1][c2];
        if (c1 != r2){
            System.out.println("Invalid dimensions");
        }
        else {
            for (int i=0; i<r1; i++){
                for (int j=0; j<c2; j++){
                    for (int k=0; k<c1; k++){
                        c[i][j] += (a[i][k] * b[k][j]);
                    }
                }
            }
        }
        return c;
    }
}

