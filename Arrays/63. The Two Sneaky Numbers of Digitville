class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int n = nums.length;
        HashMap<Integer, Integer> map = new HashMap<>();
        int [] ans = new int[2];
        int k = 0;

        for (int i=0; i<n; i++) {
            map.put(nums[i], map.getOrDefault(nums[i], 0) + 1);

            int freq = map.get(nums[i]);

            if (freq == 2) {
                ans[k++] = nums[i];
            }
        }
        return ans;
    }
}
