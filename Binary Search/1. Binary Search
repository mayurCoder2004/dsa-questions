class Solution {
    public int search(int[] nums, int target) {
        int n = nums.length;
        int st = 0, end = n-1;

        while (st <= end){
            int mid = (st + end)/2;

            if (nums[mid] > target){
                end--;
            }
            else if (nums[mid] < target) {
                st++;
            }
            else {
                return mid;
            }
        }
        return -1;
    }
}
