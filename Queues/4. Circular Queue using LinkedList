class Main {
    public static class Node {
        int val;
        Node next;
        Node(int val) {
            this.val = val;
        }
    }
    public static class QueueLL {
        Node head = null;
        Node tail = null;
        int size = 0;
        
        public void push(int x) {
            Node temp = new Node(x);
            if (head == null) {
                head = temp;
                tail = temp;
                tail.next = head;
            }
            else {
                tail.next = temp;
                tail = temp;
                tail.next = head;
            }
            size++;
        }
        
        public void remove() {
            if (head == null) {
                System.out.println("Queue is empty!");
                return;
            }
            else {
                head = head.next;
                tail.next = head;
            }
            size--;
        }
        
        public boolean isEmpty() {
            if (head == null) {
                return true;
            }
            return false;
        }
        
        public void display() {
            Node temp = head;
            
            while (temp != tail) {
                System.out.print(temp.val + " ");
                temp = temp.next;
            }
            System.out.println();
        }
    }
    public static void main(String[] args) {
        QueueLL q = new QueueLL();

        // Test 1: Check empty queue
        System.out.println("Is queue empty? " + q.isEmpty());
        q.display();

        // Test 2: Add elements
        q.push(10);
        q.push(20);
        q.push(30);
        q.push(40);
        q.display();

        // Test 3: Remove elements
        q.remove(); // removes 10
        q.display();

        // Test 4: Remove until empty
        q.remove(); // removes 20
        q.remove(); // removes 30
        q.remove(); // removes 40
        q.display();

        // Test 5: Try removing from empty queue
        q.remove();

        // Test 6: Add again after empty
        q.push(50);
        q.push(60);
        q.display();

        // Test 7: Check isEmpty again
        System.out.println("Is queue empty? " + q.isEmpty());
    }
}
