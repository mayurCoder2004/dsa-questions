                                                          // APPROACH 1

class MyQueue {
    Stack<Integer> st = new Stack<>();
    Stack<Integer> helper = new Stack<>();
    public MyQueue() {
        
    }
    
    public void push(int x) {
        st.push(x);
    }
    
    public int pop() {
        while (st.size() > 1) {
            helper.push(st.pop());
        }
        int val = st.pop();
        while (!helper.isEmpty()) {
            st.push(helper.pop());
        }
        return val;
    }
    
    public int peek() {
        while (st.size() > 1) {
            helper.push(st.pop());
        }
        int val = st.peek();
        while (!helper.isEmpty()) {
            st.push(helper.pop());
        }
        return val;
    }
    
    public boolean empty() {
        return st.isEmpty();
    }
}
